class Solution:
    def shortestPalindrome_timeout(self, s: str) -> str:
        for j in range(len(s) - 1, -1, -1):
            start, end = 0, j
            while start < end:
                if s[start] == s[end]:
                    start += 1
                    end -= 1
                else:
                    break
            if start >= end:
                return s[j + 1:][::-1] + s
        return s[1:][::-1] + s